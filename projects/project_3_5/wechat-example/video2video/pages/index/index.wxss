.page_img2video {
  position: relative;
  width: 100vw;
  display: flex;
  margin-top: 80px;
  margin-bottom: 32rpx;
  flex-direction: column;
  align-items: center;
}
.video_box {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 60vh;
  /* padding: 30px; */
}
.button_box button {
  width: 280rpx;
}

.button_box {
  width: 100%;
  padding: 40px;
  display: flex;
  justify-content: space-around;
  box-sizing: border-box;
}

.picker {
  padding: 10px;
  background-color: #f6f6f6;
  border-radius: 5px;
  border-radius: 10px;
  margin-top: 40px;
}
.output_box {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}



.output_video {
  width: 590rpx;
  height: 787rpx;
}
